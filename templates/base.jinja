{% import "components/navbar.jinja" as navigation %}
{% import "components/footer.jinja" as footerr %}

<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no">
    <title>Roomies</title>
    <link rel="stylesheet" href="/static/bootstrap/css/bootstrap.min.css">
    <link rel="stylesheet"
        href="https://fonts.googleapis.com/css?family=Inter:300italic,400italic,600italic,700italic,800italic,400,300,600,700,800&amp;display=swap">
    <link rel="stylesheet" href="/static/css/style.css">
    <script src="https://kit.fontawesome.com/c3802dc855.js" crossorigin="anonymous"></script>
    <script src="https://unpkg.com/@lottiefiles/lottie-player@latest/dist/lottie-player.js"></script>
</head>

<body>
    <!-- Start: Navbar -->
    {{ navigation.navbar() }}
    <!-- End: Navbar -->
    {% block content%}
    {% endblock %}
    <!-- Start: Footer Multi Column -->
    {{ footerr.footer() }}
    <!-- End: Footer Multi Column -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/js/bootstrap.bundle.min.js"></script>
    <script src="/static/js/bold-and-bright.js"></script>
    {% block script %}
    {% endblock %}
</body>

</html>